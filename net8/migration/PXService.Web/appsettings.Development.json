{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "WCF": {
    "Bindings": {
      "basicHttpBinding": {
        "BasicHttpBinding_ICommerceAccountService": {
          "name": "BasicHttpBinding_ICommerceAccountService",
          "maxReceivedMessageSize": 65536,
          "security": {
            "mode": "Transport"
          }
        }
      }
    },
    "Endpoints": {
      "ICommerceAccountService": {
        "address": "https://your-service-endpoint-url/CommerceAccountService.svc",
        "binding": "basicHttpBinding",
        "bindingConfiguration": "BasicHttpBinding_ICommerceAccountService",
        "contract": "ICommerceAccountService"
      }
    },
    "Client": {
      "EndpointBehaviors": {
        "PXClientBehavior": {
          "CustomHeaders": {
            "x-ms-correlation-id": "auto",
            "x-ms-tracking-id": "auto"
          }
        }
      }
    },
    "PXServiceSettings": {
      "ValidateCors": true,
      "CorsAllowedOrigins": [
        "https://localhost:5001"
      ],
      "ApplicationInsightInstrumentKey": "",
      "ApplicationInsightMode": false,
      "PIDLDocumentValidationEnabled": false
    }
  }
}
